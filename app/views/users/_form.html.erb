<%= form_for [@room,@user] do |form| %>
  Name:<br /><%= form.text_field :name %><br />
  Last Name:<br /><%= form.text_field :last_name %><br />
  Card Number: <%= form.text_field :cardnumber %><br />
  Name On Card: <%= form.text_field :namecard %><br />
  Expiration Date:<%= form.date_field :expirationdate %><br />
  CVV:<%= form.text_field :cvv %><br />
  Profile Photo:<%= form.file_field :profile_photo %><br />
  <%= form.submit %>
<% end %>