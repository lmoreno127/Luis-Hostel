<h1>Show Booking</h1>
Room:<%=@booking.room.title %>
Guests:<%=@booking.guests %>
Check-in:<%=@booking.check_in %>
Check-Out:<%=@booking.check_out %>
<% if user_signed_in? && current_user.is_admin %>
 <%= link_to 'Edit Booking', edit_room_user_booking_path(@booking.room,current_user,@booking) %>
<%end%> 
<%= link_to 'Add Review', new_room_user_review_path(@booking.room,current_user) %>
<%= link_to 'Cancel Booking', room_user_booking_path(@booking.room,current_user,@booking), method: :delete %>