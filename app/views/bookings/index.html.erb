<h1>Your Bookings</h1>
<ul>
  <% @bookings.each do |booking| %>
  <li>
    Room :<%= booking.room.title%>  
    <%= link_to 'Show Booking', room_user_booking_path(booking.room,current_user,booking) %>
    <%= link_to 'Cancel Booking', room_user_booking_path(booking.room,current_user,booking), method: :delete %>
  </li>
  <%end %>
</ul>

